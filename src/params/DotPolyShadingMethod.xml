<magics>
	<class inherits="PolyShadingMethod"
		xmltag="dot"
		name="DotPolyShadingMethod"
		directory="visualisers"
		prefix="contour/contour/contour_shade/contour_shade/contour_shade"
		action="pcont"
		python="mcont"
		doc_inherits="no">
		<parameter member="size"
			to="float"
			default="0.02"
			from="float"
			name="contour_shade_dot_size">
			<documentation>Size of dot in shading pattern</documentation>
		</parameter>
		<parameter member="max_density"
			to="float"
			default="50.0"
			from="float"
			name="contour_shade_max_level_density">
			<documentation>Dots/square centimetre in highest shading band</documentation>
		</parameter>
		<parameter xml="min_level_density"
			from="float"
			name="contour_shade_min_level_density"
			default="1.0"
			member="min_density"
			to="float">
			<documentation>Dots/square centimetre in lowest shading band</documentation>
		</parameter>
	</class>
</magics>
