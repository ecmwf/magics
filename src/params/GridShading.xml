<magics>
	<class inherits="PolyShadingTechnique"
		top="ShadingTechnique"
		name="GridShading"
		directory="visualisers"
		action="pcont"
		python="mcont"
		include="PolyShadingTechnique.h"
		doc_inherits="no">
		<parameter from="string"
			name="contour_grid_shading_position"
			default="middle"
			member="position"
			to="string"
			values="middle/bottom_left">
			<documentation>Middle : the point is in the midlle of the cell, bottom_left : the point is in the bottom left corner</documentation>
		</parameter>
	</class>
</magics>
