         		<release>0.1		</release>
	</parameter>

	<parameter xml="lo_colour" from="string" name="contour_lo_colour" default="blue" member="lo_colour" to="Colour">
         		<documentation> Colour of local minima text or number 		</documentation>
         		<release>0.1		</release>
	</parameter>
